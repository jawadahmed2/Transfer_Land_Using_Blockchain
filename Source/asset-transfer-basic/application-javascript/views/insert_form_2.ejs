<!DOCTYPE html>
<html lang="en">

<head>
    <title>Add Land Record</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="login_stylesheet/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="login_stylesheet/css/util.css">
    <link rel="stylesheet" type="text/css" href="login_stylesheet/css/main.css">
    <!--===============================================================================================-->
</head>

<body>
    <%if(errors.length> 0) {%>
        <div class="alert alert-danger">
            <%=errors%>
        </div>
        <%}%>
            <%if(success.length> 0) {%>
                <div class="alert alert-success">
                    <%=success%>
                </div>
                <%}%>
                    <%# To check server-side validations, remove needs-validation class temporarily%>

                        <div class="container" style="margin-top: -3.9%;">
                            <div class="limiter">
                                <div class="container-login100">
                                    <div class="wrap-login100 p-l-55 p-r-55 p-t-20 p-b-35">
                                        <form action="/create_asset" method="POST" class="login100-form validate-form" novalidate>

                                            <span class="login100-form-title p-b-5">
                                                Register Land
                                            </span>

                                            <div class="wrap-input100 validate-input m-b-5 "
                                                data-validate="Username is reauired">
                                                <span class="label-input100 m-b-5">Land ID</span>
                                                <input class="input100" type="text" name="id" placeholder="Land ID">
                                                <span class="focus-input100" data-symbol="&#9958;"></span>
                                            </div>

                                            <div class="wrap-input100 validate-input m-b-5"
                                                data-validate="Username is reauired">
                                                <span class="label-input100 m-b-5">Address</span>
                                                <input class="input100" type="text" name="address"
                                                    placeholder="Islamabad">
                                                <span class="focus-input100" data-symbol="&#9958;"></span>
                                            </div>
                                            <!-- <div class="invalid-feedback">
                                                Please enter user address
                                            </div> -->

                                            <div class="wrap-input100 validate-input m-b-5"
                                                data-validate="Username is reauired">
                                                <span class="label-input100 m-b-5">Land Size</span>
                                                <input class="input100" type="text" name="size" placeholder="123 sq ft">
                                                <span class="focus-input100" data-symbol="&#9958;"></span>
                                            </div>

                                            <div class="wrap-input100 validate-input m-b-5"
                                                data-validate="Username is reauired">
                                                <span class="label-input100 m-b-5">Land Price</span>
                                                <input class="input100" type="text" name="value" placeholder="1234">
                                                <span class="focus-input100" data-symbol="&#xf206;"></span>
                                            </div>



                                            <input class="wrap-input100 validate-input m-t-25" type="file" name="file"
                                                value="images/about_us.png">


                                            <div class="container-login100-form-btn p-t-15 p-b-5">
                                                <div class="wrap-login100-form-btn">
                                                    <div class="login100-form-bgbtn"></div>
                                                    <button class="login100-form-btn" type="submit" value="Add Asset">
                                                        Submit
                                                    </button>
                                                </div>
                                            </div>


                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div id="dropDownSelect1"></div>

                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/jquery/jquery-3.2.1.min.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/animsition/js/animsition.min.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/bootstrap/js/popper.js"></script>
                        <script src="login_stylesheet/vendor/bootstrap/js/bootstrap.min.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/select2/select2.min.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/daterangepicker/moment.min.js"></script>
                        <script src="login_stylesheet/vendor/daterangepicker/daterangepicker.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/vendor/countdowntime/countdowntime.js"></script>
                        <!--===============================================================================================-->
                        <script src="login_stylesheet/js/main.js"></script>

</body>

</html>